<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 以上为固定写法 -->

<mapper namespace="com.RailManager.demo.mapper.PermissionMapper">

    <select id="getPermission" resultType="com.RailManager.demo.model.Permission">
        SELECT *
        FROM "Permission"
        WHERE "userId" = #{userId}
    </select>

</mapper>